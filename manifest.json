{
  "manifest_version": 2,
  "name": "DY Affinity Extension",
  "version": "2020.10.26",
  "content_scripts": [
    {
      "matches": ["*://*.urbanoutfitters.com/*"],
      "js": ["script.js"],
      "css": ["styles.css"]
    }
  ],
  "permissions": ["webRequest", "webRequestBlocking", "<all_urls>"],
  "background": { "scripts": ["background.js"] }
}
